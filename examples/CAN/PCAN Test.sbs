using StepBro.CAN;

procedure void OpenCAN()
{
    var channel = PCAN.Driver.GetAdapter("USBBUS1").GetChannel(0);
    channel.Setup(Baudrate.BR500K, ChannelMode.Extended);
    if (!channel.IsOpen)
    {
        channel.Open();
    }
	var statusQueue = channel.CreateReceiveQueue(m => m.ID == 0x1092);
}
